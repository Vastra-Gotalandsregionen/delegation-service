<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
                           http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

  <!--<import resource="messagebus-common.xml"/>-->

  <context:annotation-config/>
  <context:spring-configured/>

  <camelContext xmlns="http://camel.apache.org/schema/spring">

    <routeBuilder ref="delegationSigningResultRouteBuilder"/>

  </camelContext>

  <!-- RouteBuilders -->
  <bean id="delegationSigningResultRouteBuilder" class="se.vgregion.routes.DelegationSigningResultRouteBuilder">
    <constructor-arg value="${response.url}" />
  </bean>

</beans>